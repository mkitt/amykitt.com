{
  "name": "amykitt",
  "description": "Web site for Amy Kitt",
  "homepage": "https://amykitt.com/",
  "version": "3.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "next build && next export",
    "chex": "npx tsc && npm run lint",
    "deploy": "gh-pages --branch site --dist out --dotfiles",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start"
  },
  "dependencies": {
    "next": "11.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intersection-observer": "8.32.0"
  },
  "devDependencies": {
    "@types/node": "15.12.4",
    "@types/react": "17.0.13",
    "autoprefixer": "10.2.6",
    "eslint": "7.29.0",
    "eslint-config-next": "11.0.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "gh-pages": "3.2.2",
    "postcss": "8.3.5",
    "prettier": "2.3.1",
    "tailwindcss": "2.2.2",
    "typescript": "4.3.4"
  },
  "eslintConfig": {
    "extends": [
      "plugin:jsx-a11y/strict",
      "next",
      "next/core-web-vitals"
    ]
  },
  "postcss": {
    "plugins": [
      "tailwindcss",
      "autoprefixer"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
