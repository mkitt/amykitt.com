{
  "name": "amykitt",
  "description": "Web site for Amy Kitt",
  "homepage": "https://amykitt.com/",
  "version": "3.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "next build && next export",
    "chex": "yarn tsc",
    "deploy": "gh-pages --branch site --dist out --dotfiles",
    "format": "prettier --ignore-path ./.gitignore --write .",
    "dev": "next dev",
    "start": "next start"
  },
  "dependencies": {
    "next": "10.2.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intersection-observer": "8.32.0"
  },
  "devDependencies": {
    "@types/node": "15.6.0",
    "@types/react": "17.0.6",
    "autoprefixer": "10.2.5",
    "gh-pages": "3.1.0",
    "postcss": "8.3.0",
    "prettier": "2.3.0",
    "tailwindcss": "2.1.2",
    "typescript": "4.2.4"
  },
  "postcss": {
    "plugins": [
      "tailwindcss",
      "autoprefixer"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
